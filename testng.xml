<?xml version="1.0" encoding="UTF-8"?>
<suite name="Plancess Web APP Suite" parallel="false">

	<listeners>
		<listener class-name="com.plancess.selenium.reporter.PlancessReporter" />
	</listeners>


	<parameter name="browser" value="firefox" />
	<parameter name="os" value="LINUX" />
	<!-- <parameter name="host_ip" value="phabricator.plancess.com" /> -->
	<test name="WebApp Test with Firefox on Linux">
		<classes>
			<class name="com.plancess.selenium.tests.BaseTest" />

			<class name="com.plancess.selenium.tests.LoginFromLandingPageTest" />
			<class name="com.plancess.selenium.tests.SignUpFromLandingPageTest" />

		</classes>
	</test>

</suite> <!-- Suite -->

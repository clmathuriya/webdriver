<?xml version="1.0" encoding="UTF-8"?>
<suite name="Preplane Web APP Suite" parallel="false">

	<listeners>
		<listener class-name="com.plancess.selenium.reporter.PlancessReporter" />
	</listeners>


	<parameter name="browser" value="firefox" />
	<parameter name="os" value="LINUX" />
	<!-- <parameter name="host_ip" value="chhaganmathuriya:135ce8d8-f5af-427c-a4cd-76b2d6c50dfc@ondemand.saucelabs.com" 
		/> <parameter name="port" value="80" /> -->
	<test name="WebApp Smoke Test with 
		Firefox on Linux">
		<classes>
			<class name="com.plancess.selenium.tests.BaseTest" />
			<class name="com.plancess.selenium.tests.LoginFromLandingPageTest">
				<methods>
					<include name="loginWithValidEmailPasswordTest" />
					<include name="loginWithFacebookSuccessTest" />
					<include name="loginWithGoogleSuccessTest" />
				</methods>
			</class>

			<class name="com.plancess.selenium.tests.DashboardTest" />
			<class name="com.plancess.selenium.tests.ResetPasswordTest" />
			<class name="com.plancess.selenium.tests.AssessmentTest">
				<methods>
					<include name="noMoreTestErorTest" />

				</methods>
			</class>
			<class name="com.plancess.selenium.tests.UserProfileTest" />
			<class name="com.plancess.selenium.tests.UserSecurityTest" />
		</classes>
	</test>



</suite> <!-- Suite -->
